!false // true

var a = 1;

3 == "3"; // true se fija en el valor de la comparacion

3 === "3"; //false se fija en el tipo del valor

5 < 3; // false

5 > 3; // true

5 <= 6 // true

5 >= 6 //false

a && b // si las dos son iguales sea falso o verdadero dara un "true"

a || b // si cualquiera de las dos condiciones es true. dara "true"

var edad = 40;

edad++ // 40+1 = 41

edad += 2; // 40+2 = 42